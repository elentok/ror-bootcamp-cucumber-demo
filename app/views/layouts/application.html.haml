!!!5
%html
%head
  %title App
  = stylesheet_link_tag    "application", :media => "all"
  = javascript_include_tag "application"
  = csrf_meta_tags

%body
  .navbar
    .navbar-inner
      .container
        %a.brand{href: root_url} App

  .container
    .row
      - unless notice.nil?
        #notice.alert= notice
      = yield
